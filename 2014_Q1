#include <iostream>
#include <vector>

using namespace std;

int main() {
	int k, m;
	
	cin >> k;
	cin >> m;
	
	vector <int> friends;
	int rounds[m]; //array
	
	for (int x = 0; x < k;x++) {
		int y=x+1;
		friends.push_back (y);
	}
	
	for (int y = 0; y < a; y++) {
		int delete = rounds[y] - 1; //starts with the number at point r, adjust to position
		while (delete < friends.size()) {
			//erase vector[delete]
			
			delete = delete + (a - 1)
		}
		
	}
	
	cout << sum << endl;
		
	return 0;
}
